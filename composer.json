{
    "name": "xamplifier/reviews",
    "description": "Fetch vendor reviews such as Google, Facebook and maybe others.",
    "type": "library",
    "repositories":
    [
            {
                "type": "vcs",
                "url": "https://github.com/xamplifier/php-graph-sdk"
            }
    ],
    "require": {
        "php": "^8.0",
        "google/apiclient": "^2.12.1",
        "nickdnk/graph-sdk": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.11",
        "larapack/dd": "^1.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Xamplifier\\Reviews\\": "src/"
        }
    },
    "autoload-dev": {
          "psr-4": {
             "Xamplifier\\Reviews\\Test\\": "tests"
         },
        "authors": [
            {
                "name": "Antonis Flangofas",
                "email": "antonisflags@gmail.com"
            }
        ]
    }
}
